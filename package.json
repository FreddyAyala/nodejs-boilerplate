{
  "name": "nodejs-babel-vscode-debug",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "run-s build serve",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "build": "babel . -d dist --copy-files --source-maps --ignore node_modules,dist,package.json,package-lock.json,yarn.lock",
    "dev": "cross-env NODE_ENV=development nodemon index.js --exec babel-node"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "cross-env": "5.0.1",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "sequelize-cli": "2.7.0"
  }
}
